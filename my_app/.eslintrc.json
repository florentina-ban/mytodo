{
    "extends": ["eslint:all", "plugin:react/all"],
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "warn"
    }
  }